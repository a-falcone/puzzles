#!/usr/bin/perl

use strict;
use warnings;

my ( @r, $original, %new );

while (<DATA>) {
    chomp;
    next unless $_;
    if ( my ($src, $dest) = /^(.*) => (.*)$/ ) {
        push @r, [$src, $dest];
    } else {
        $original = $_;
    }
}

for my $i ( 0..(length($original) - 1) ) {
    for ( @r ) {
        my $first = substr $original, 0, $i;
        my $last = substr $original, $i;
        $last =~ s/$_->[0]/$_->[1]/;
        $new{"$first$last"}++;
    }
}

delete $new{$original};

print scalar keys %new;
print "\n";

__DATA__
