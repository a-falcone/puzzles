#!/usr/bin/perl

use strict;
use warnings;

my ( @r, $original );

while (<DATA>) {
    chomp;
    next unless $_;
    if ( my ($src, $dest) = /^(.*) => (.*)$/ ) {
        push @r, [$src, $dest];
    } else {
        $original = $_;
    }
}

print reduce( $original ) . "\n";

sub reduce {
    my $target = shift;
    return 0 if $target eq "e";

    for ( @r ) {
        my $copy = $target;
        my $result;
        if ( $copy =~ s/$_->[1]/$_->[0]/ ) {
            $result = reduce($copy);
            if ( $result >= 0 ) {
                return ($result + 1);
            }
        }
    }
    
    return -1;
}

__DATA__
Al => ThF
Al => ThRnFAr
B => BCa
B => TiB
B => TiRnFAr
