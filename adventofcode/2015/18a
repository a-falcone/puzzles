#!/usr/bin/perl

use strict;
use warnings;

my $b = [];
my $birth = '3$';
my $survive = '^[23]$';

while (<DATA>) {
    chomp;
    push @$b, [map { /#/ ? 1 : 0 } split( //, $_ )];
}

for ( 1..100 ) {
    $b = evolve( $b );
}

my $count = 0;

for my $row ( @$b ) {
    for ( @$row ) {
        $count++ if $_;
    }
}

print "$count\n";

sub evolve {
    my $b = $_[0];
    my $newb = [];
    my $maxy = scalar( @$b ) - 1;
    my $maxx = scalar( @{ $b->[0] } ) - 1;
    for my $y ( 0..$maxy ) {
        for my $x ( 0..$maxx ) {
            my $neighbors = 0;
            
            my $left = $x - 1;
            my $right = $x + 1;
            my $up = $y - 1;
            my $down = $y + 1;
            $neighbors++ if $b->[$left][$up] && $left >= 0 && $up >= 0;
            $neighbors++ if $b->[$x][$up] && $up >= 0;
            $neighbors++ if $b->[$right][$up] && $right <= $maxx && $up >= 0;
            $neighbors++ if $b->[$left][$y] && $left >= 0;
            $neighbors++ if $b->[$right][$y] && $right <= $maxx;
            $neighbors++ if $b->[$left][$down] && $left >= 0 && $down <= $maxy;
            $neighbors++ if $b->[$x][$down] && $down <= $maxy;
            $neighbors++ if $b->[$right][$down] && $right <= $maxx && $down <= $maxy;

            if ( $b->[$x][$y] ) {
                $newb->[$x][$y] = $neighbors =~ /$survive/;
            } else {
                $newb->[$x][$y] = $neighbors =~ /$birth/;
            }
        }
    }
    return $newb;
}

__DATA__
